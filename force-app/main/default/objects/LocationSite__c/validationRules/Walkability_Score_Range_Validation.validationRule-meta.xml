<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Walkability_Score_Range_Validation</fullName>
    <active>true</active>
    <description>Ensures walkability score is between 1-100</description>
    <errorConditionFormula>AND(
  NOT(ISBLANK(Walkability_score_1_100__c)),
  OR(
    Walkability_score_1_100__c &lt; 1,
    Walkability_score_1_100__c &gt; 100
  )
)</errorConditionFormula>
    <errorDisplayField>Walkability_score_1_100__c</errorDisplayField>
    <errorMessage>Walkability Score must be between 1 and 100</errorMessage>
</ValidationRule>
